{
  "$schema": "vscode://schemas/color-theme",
	"name": "AGNIS base",
  "include": "./agnis-ui.json",
  "type": "dark",
  "semanticHighlighting": true,  
  "semanticTokenColors": {
    "newOperator": "#CDD0CA",
    "stringLiteral": "#a38f70",
    "customLiteral": "#CDD0CA",
    "numberLiteral": "#df99ff",
  },
  "tokenColors": [
		{
			"scope": [
				"source",
			],
			"settings": {
				"foreground": "#CDD0CA"
			}
		},
		{
			"scope": [
				"meta.embedded",
				"source.groovy.embedded"
			],
			"settings": {
				"foreground": "#CDD0CA"
			}
		},
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "header",
			"settings": {
				"foreground": "#000080"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#6E7164"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#df99ff"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"variable.other.enummember",
				"keyword.operator.plus.exponent",
				"keyword.operator.minus.exponent"
			],
			"settings": {
				"foreground": "#df99ff" // 9BCF23
			}
		},
		{
			"scope": "constant.regexp", // TODO
			"settings": {
				"foreground": "#D4FF70" // TODO
			}
		},    
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#85ac3d", // TODO
        "fontStyle": ""
			}
		},
		{
			"scope": "entity.name.tag.css",
			"settings": {
				"foreground": "#d3ff35", // TODO
        "fontStyle": ""
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#f2e2ae"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name.class.css",
				"entity.other.attribute-name.class.mixin.css",
				"entity.other.attribute-name.id.css",
				"entity.other.attribute-name.parent-selector.css",
				"entity.other.attribute-name.pseudo-class.css",
				"entity.other.attribute-name.pseudo-element.css",
				"source.css.less entity.other.attribute-name.id",
				"entity.other.attribute-name.scss"
			],
			"settings": {
				"foreground": "#23B5CF"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#f04400"
			}
		},
		{
			"scope": "meta.paragraph.markdown",
			"settings": {
				"foreground": "#CDD0CA",
			}
		},
		{
			"scope": "string.other.link.title.markdown",
			"settings": {
				"foreground": "#23B5CF",
			}
		},
		{
			"scope": "meta.link.inline.markdown",
			"settings": {
				"foreground": "#A38F70",
			}
		},
		{
			"scope": "markup.underline.link.markdown",
			"settings": {
				"foreground": "#256F6E",
			}
		},
		{
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#A2A599"
			}
		},
		{
			"scope": "markup.heading",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#d4e448"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#C3E88D"
			}
		},
		{
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"scope": "markup.changed",
			"settings": {
				"foreground": "#C792EA"
			}
		},
		{
			"scope": "punctuation.definition.quote.begin.markdown",
			"settings": {
				"foreground": "#a38f70" // TODO
			}
		},
		{
			"scope": "punctuation.definition.list.begin.markdown",
			"settings": {
				"foreground": "#f6d256"
			}
		},
		{
			"scope": "markup.inline.raw",
			"settings": {
				"foreground": "#a38f70" // TODO
			}
		},
		{
			"name": "brackets of XML/HTML tags",
			"scope": "punctuation.definition.tag",
			"settings": {
				"foreground": "#A9ADA7"
			}
		},
		{
			"name": "text in HTML",
			"scope": "text.html.derivative",
			"settings": {
				"foreground": "#CDD0CA"
			}
		},
		{
			"scope": [
				"meta.preprocessor",
				"entity.name.function.preprocessor"
			],
			"settings": {
				"foreground": "#a8aaa5" // TODO
			}
		},
		{
			"scope": "meta.preprocessor.string",
			"settings": {
				"foreground": "#a38f70" // TODO
			}
		},
		{
			"scope": "meta.preprocessor.numeric",
			"settings": {
				"foreground": "#df99ff" // TODO
			}
		},
		{
			"scope": "meta.structure.dictionary.key.python",
			"settings": {
				"foreground": "#CDD0CA" // TODO
			}
		},
		{
			"scope": "meta.diff.header",
			"settings": {
				"foreground": "#f6d256" // TODO
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#f6d256"
			}
		},
		{
			"scope": [
        "storage.type",
        "keyword.operator.noexcept"
      ],
			"settings": {
				"foreground": "#f6d256"
			}
		},
		{
			"scope": [
				"storage.modifier",
			],
			"settings": {
				"foreground": "#C3A644",
				"fontStyle": ""
			}
		},
		{
			"scope": [
				"string",
				"meta.embedded.assembly"
			],
			"settings": {
				"foreground": "#a38f70"
			}
		},
		{
			"scope": "string.tag",
			"settings": {
				"foreground": "#a38f70",
        "fontStyle": "bold" // TODO
			}
		},
		{
			"scope": "string.value",
			"settings": {
				"foreground": "#a38f70",
        "fontStyle": "bold" // TODO
			}
		},
		{
			"scope": "string.regexp",
			"settings": {
				"foreground": "#D4FF70"
			}
		},
		{
			"name": "String interpolation",
			"scope": [
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end",
        "punctuation.quasi.element.begin",
        "punctuation.quasi.element.end",
        "punctuation.section.embedded",
			],
			"settings": {
				"foreground": "#C9B08A",
				"fontStyle": ""
			}
		},
		{
			"name": "Reset JavaScript string interpolation expression",
			"scope": [
				"meta.template.expression"
			],
			"settings": {
				"foreground": "#CDD0CA"
			}
		},
		{
			"scope": [
				"support.type.vendored.property-name",
				"support.type.property-name",
				"variable.css",
				"variable.scss",
				"variable.other.less",
				"source.coffee.embedded"
			],
			"settings": {
				"foreground": "#d2d4ca"
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#f6d256"
			}
		},
		{
			"scope": "keyword.control",
			"settings": {
				"foreground": "#f6d256"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#df3c7a", 
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"keyword.operator.new",
				"keyword.operator.expression",
				"keyword.operator.cast",
				"keyword.operator.sizeof",
				"keyword.operator.alignof",
				"keyword.operator.typeid",
				"keyword.operator.alignas",
				"keyword.operator.instanceof",
				"keyword.operator.logical.python",
				"keyword.operator.wordlike"
			],
			"settings": {
				"foreground": "#df3c7a",
        "fontStyle": ""
			}
		},
		{
			"scope": "keyword.other.unit",
			"settings": {
				"foreground": "#df3c7a" // TODO
			}
		},
		{
			"scope": [
				"punctuation.section.embedded.begin.php",
				"punctuation.section.embedded.end.php"
			],
			"settings": {
				"foreground": "#df3c7a" // TODO
			}
		},
		{
			"scope": "support.function.git-rebase",
			"settings": {
				"foreground": "#9cdcfe" // TODO
			}
		},
		{
			"scope": "constant.sha.git-rebase",
			"settings": {
				"foreground": "#b5cea8" // TODO
			}
		},
		{
			"name": "coloring of the Java import and package identifiers",
			"scope": [
				"storage.modifier.import.java",
				"variable.language.wildcard.java",
				"storage.modifier.package.java"
			],
			"settings": {
				"foreground": "#CDD0CA"
			}
		},
		{
			"name": "this.self",
			"scope": "variable.language",
			"settings": {
				"foreground": "#A9ADA7",
        "fontStyle": "italic"
			}
		}

  ]
}