{
  "$schema": "vscode://schemas/color-theme",
  "name": "AGNIS base",
  "include": "./agnis-ui.json",
  "type": "dark",
  "tokenColors": [
    {
      "scope": [
        "source",
      ],
      "settings": {
        "foreground": "#CDD0CA"
      }
    },
    {
      "scope": [
        "meta.embedded",
        "source.groovy.embedded"
      ],
      "settings": {
        "foreground": "#CDD0CA"
      }
    },
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "header",
      "settings": {
        "foreground": "#4fc7dc"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#8B8E81"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#df99ff"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "variable.other.enummember",
        "keyword.operator.plus.exponent",
        "keyword.operator.minus.exponent"
      ],
      "settings": {
        "foreground": "#df99ff" // 9BCF23
      }
    },
    {
      "scope": "constant.regexp", // TODO
      "settings": {
        "foreground": "#D4FF70" // TODO
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#b1b4a7",
        "fontStyle": ""
      }
    },
    {
      "scope": "entity.name.tag.css",
      "settings": {
        "foreground": "#b1b4a7",
        "fontStyle": ""
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#f2e2ae"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.class.mixin.css",
        "entity.other.attribute-name.id.css",
        "entity.other.attribute-name.parent-selector.css",
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-element.css",
        "source.css.less entity.other.attribute-name.id",
        "entity.other.attribute-name.scss"
      ],
      "settings": {
        "foreground": "#36aabf"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#f04400"
      }
    },
    {
      "scope": "meta.paragraph.markdown",
      "settings": {
        "foreground": "#CDD0CA",
      }
    },
    {
      "scope": "string.other.link.title.markdown",
      "settings": {
        "foreground": "#36aabf",
      }
    },
    {
      "scope": "meta.link.inline.markdown",
      "settings": {
        "foreground": "#A38F70",
      }
    },
    {
      "scope": "markup.underline.link.markdown",
      "settings": {
        "foreground": "#256F6E",
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#E1E4DE"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#d4e448"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "scope": "punctuation.definition.quote.begin.markdown",
      "settings": {
        "foreground": "#b1b4a7"
      }
    },
    {
      "scope": "punctuation.definition.list.begin.markdown",
      "settings": {
        "foreground": "#e0c871"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#b1b4a7"
      }
    },
    {
      "name": "brackets of XML/HTML tags",
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#A9ADA7"
      }
    },
    {
      "name": "text in HTML",
      "scope": "text.html.derivative",
      "settings": {
        "foreground": "#CDD0CA"
      }
    },
    {
      "scope": [
        "meta.preprocessor",
        "entity.name.function.preprocessor"
      ],
      "settings": {
        "foreground": "#a8aaa5" // TODO
      }
    },
    {
      "scope": "meta.preprocessor.string",
      "settings": {
        "foreground": "#a38f70" // TODO
      }
    },
    {
      "scope": "meta.preprocessor.numeric",
      "settings": {
        "foreground": "#df99ff" // TODO
      }
    },
    {
      "scope": "meta.structure.dictionary.key.python",
      "settings": {
        "foreground": "#CDD0CA" // TODO
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#e0c871" // TODO
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "#d1c082"
      }
    },
    {
      "scope": [
        "storage.type",
        "keyword.operator.noexcept"
      ],
      "settings": {
        "foreground": "#d1c082"
      }
    },
    {
      "scope": [
        "storage.modifier",
      ],
      "settings": {
        "foreground": "#d1c082",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "string",
        "meta.embedded.assembly"
      ],
      "settings": {
        "foreground": "#a38f70"
      }
    },
    {
      "scope": "string.tag",
      "settings": {
        "foreground": "#a38f70",
        "fontStyle": "bold" // TODO
      }
    },
    {
      "scope": "string.value",
      "settings": {
        "foreground": "#a38f70",
        "fontStyle": "bold" // TODO
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#D4FF70"
      }
    },
    {
      "name": "String interpolation",
      "scope": [
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "punctuation.quasi.element.begin",
        "punctuation.quasi.element.end",
        "punctuation.section.embedded",
      ],
      "settings": {
        "foreground": "#C9B08A",
        "fontStyle": ""
      }
    },
    {
      "name": "Reset JavaScript string interpolation expression",
      "scope": [
        "meta.template.expression"
      ],
      "settings": {
        "foreground": "#CDD0CA"
      }
    },
    {
      "scope": [
        "support.type.vendored.property-name",
        "support.type.property-name",
        "variable.css",
        "variable.scss",
        "variable.other.less",
        "source.coffee.embedded"
      ],
      "settings": {
        "foreground": "#d2d4ca"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#e0c871"
      }
    },
    {
      "scope": "keyword.control",
      "settings": {
        "foreground": "#e0c871"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#da6391",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "punctuation.accessor.optional",
      "settings": {
        "foreground": "#fff",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "keyword.operator.assignment",
        "keyword.operator.spread",
      ],
      "settings": {
        "foreground": "#a9aca8",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.cast",
        "keyword.operator.sizeof",
        "keyword.operator.alignof",
        "keyword.operator.typeid",
        "keyword.operator.alignas",
        "keyword.operator.instanceof",
        "keyword.operator.logical.python",
        "keyword.operator.wordlike"
      ],
      "settings": {
        "foreground": "#df3c7a",
        "fontStyle": ""
      }
    },
    {
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#df3c7a" // TODO
      }
    },
    {
      "scope": [
        "punctuation.section.embedded.begin.php",
        "punctuation.section.embedded.end.php"
      ],
      "settings": {
        "foreground": "#df3c7a" // TODO
      }
    },
    {
      "scope": "support.function.git-rebase",
      "settings": {
        "foreground": "#9cdcfe" // TODO
      }
    },
    {
      "scope": "constant.sha.git-rebase",
      "settings": {
        "foreground": "#b5cea8" // TODO
      }
    },
    {
      "name": "coloring of the Java import and package identifiers",
      "scope": [
        "storage.modifier.import.java",
        "variable.language.wildcard.java",
        "storage.modifier.package.java"
      ],
      "settings": {
        "foreground": "#CDD0CA"
      }
    },
    {
      "name": "this.self",
      "scope": "variable.language",
      "settings": {
        "foreground": "#A9ADA7",
        "fontStyle": "italic"
      }
    }
  ]
}