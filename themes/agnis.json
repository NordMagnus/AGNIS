  "semanticTokenColors": {
    // --- General semantic tokens (OneDark-Pro compatible) ---
    "variable": {
      "fontStyle": "italic"
    },
    "parameter": "#E1E4DE",
    "property": "#A2A599",
    "function": "#36aabf",
    "method": "#36aabf",
    "class": "#B1B4A7",
    "enum": "#5caad4",
    "interface": "#4fc7dc",
    "type": "#4fc7dc",
    "namespace": "#4fc7dc",
    "typeParameter": "#5caad4",
    "enumMember": "#4fc7dc",
    "event": "#FBD549",
    "modifier": "#d1c082",
    "regexp": "#d4ff70",
    "decorator": "#F2E2AE",
    "macro": "#F2E2AE",
    "struct": "#4fc7dc",
    "union": "#4fc7dc",
    "label": "#A2A599",
    "operator": "#df3c7a",
    "keyword": "#d1c082",
    
    // --- OneDark-Pro inspired semantic tokens ---
    "annotation:dart": "#df99ff",
    "variable.readonly": "#8FA6A1",
    "property.readonly": "#8FA6A1",
    "variable.readonly.defaultLibrary": "#8FA6A1",
    "type.defaultLibrary": "#4fc7dc",
    "class.defaultLibrary": "#4fc7dc",
    "interface.defaultLibrary": "#4fc7dc",
    
    // --- Language-specific semantic tokens ---
    "newOperator": "#df3c7a",
    "stringLiteral": "#a38f70", 
    "customLiteral": "#CDD0CA",
    "numberLiteral": "#df99ff",
    
    // --- Class and type styling ---
    "class.declaration": {
      "foreground": "#EAEEE7",
      "fontStyle": "bold"
    },
    "member.declaration": {
      "foreground": "#A2A599"
    },
    "method.declaration": {
      "foreground": "#A2A599"
    },
    "type.declaration": "#A2A599",
    
    // --- Default library support ---
    "*.defaultLibrary": "#cee02e",
    "function.defaultLibrary": "#919c30",
    "variable.defaultLibrary": "#cee02e",
    "function.declaration": "#36aabf",
    
    // --- Modifiers ---
    "*.static": {
      "fontStyle": "underline"
    },
    "*.abstract": {
      "fontStyle": "italic"
    },
    "*.async": {
      "foreground": "#00d1d1"
    },
    "*.declaration.async": {
      "foreground": "#eaeee7",
      "fontStyle": ""
    },
    "*.deprecated": {
      "fontStyle": "strikethrough"
    },
    "*.readonly": {
      "fontStyle": "italic"
    },
    
